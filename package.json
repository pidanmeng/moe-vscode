    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "@vscode/test-electron": "^2.2.0",
    "mocha": "^10.1.0",
    "prettier": "^2.7.1",
    "turbo": "latest",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.11.0"
}